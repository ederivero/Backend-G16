{
  "name": "cafeteria-express",
  "version": "1.0.0",
  "description": "api de una cafeteria con permisos de usuario",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:dev": "nodemon --env-file=.env src/index.js",
